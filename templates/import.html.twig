{#
    The following variables are created in this scope:
      polymer_assets (array) An array of assets to import, e.g. ["core-icons.html", "core-menu.html"].

    As always the polymer configuration is made available through the polymer.configuration variable.
#}
{% for asset in polymer_assets %}
{{ attach_library('twig_polymer/webcomponents-lite') }}
<link rel="import" href="{{ polymer_asset(asset) }}">
{% endfor %}
